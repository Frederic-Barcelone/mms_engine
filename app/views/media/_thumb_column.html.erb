<% thumbnail = medium.thumbnail_image
   if thumbnail.nil? %>
     <td>&nbsp;</td>
<% else %>
     <td width="<%= thumbnail.width %>" height="<%= thumbnail.height %>">
<%=    link_to image_tag(thumbnail.public_filename, :width => thumbnail.width, :height => thumbnail.height), medium_path(medium), {:class => 'media-thumb thl-pop no-view-alone no-main-site-ajax padding-10', :onclick => "universal_navigation.loadTabsFor(#{medium.id})"} %>
     </td>
<% end %>