<h1><%= ts 'new.record', :what => Medium.human_name.titleize %></h1>
<%= error_messages_for :medium %>
<% form_for(:medium, :url => media_path, :html => {:multipart => true}) do |f| %>
     <p><b><%= Medium.human_name.titleize.s %></b><br />
<%=  file_field :image, :uploaded_data %></p>
<%=  render :partial => 'form', :locals => {:f => f} %>
     <p><%= globalized_submit_tag 'create.this' %></p>
<% end %>
<p>&nbsp;<br/>
<%= link_to ts('back.this'), media_path %></p>