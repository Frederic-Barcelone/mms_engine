<% if @media.nil? && @pictures.nil? && @videos.nil? && @documents.nil? %>
     $("#primary").html("<%= escape_javascript(render(:partial => 'media/main')) %>")
<% else
     if @medium_pages.nil? %>
       $("#secondary").html("<%= escape_javascript(render(:partial => 'media/index')) %>")
<%     if !element_id.blank? %>
         $("#navigation").html("<%= escape_javascript(render(:partial => @partial)) %>")
<%     end
     else %>
       $("#secondary").html("<%= escape_javascript(render(:partial => @type == 'Document' ? 'documents/paged_index' : 'media/paged_index_full')) %>")
<%   end %>
     ActivateThlPopups('#secondary');
     tb_init(a.thickbox, area.thickbox, input.thickbox);
<% end %>