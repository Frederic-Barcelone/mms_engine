<h1>Processing Importation by <%= @user.screen_name %></h1>
<%= javascript_tag do -%>
$(document).ready(
  function(){ setInterval(function(){ $.ajax({ url: "<%= status_media_imports_path(:user_id => @user.id) %>", type: "GET", dataType: "script" }); }, 6000 );
});
<% end -%>
<div id="status" class="normal">
<%= render :partial => 'status' %>
</div>
<p>&nbsp;<br/>
<%= link_to "New #{Medium.model_name.human.s} importation", new_media_import_path %> |
<%= link_to "#{Medium.model_name.human.titleize.s} Importation Home", media_imports_path %> |
<%= render :partial => 'main/back_to_admin_or_media' %>
</p>