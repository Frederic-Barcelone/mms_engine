<%= Util::MARGIN * @margin_depth %>
<%  if @child.children.blank? %>
      &#9674;
<%  else %>
<%=   link_to_remote '+', { :update => "#{@child.id}_div", :url => expand_element_path(@child.id, :margin_depth => @margin_depth), :method => 'get' }, { :class => 'collapsable' } %>
<%  end %>
<%= link_to_remote h(@child.title).s, :url => element_path(@child), :method => 'get' %>
<%  not_base_language do %>
      (<%= link_to ts('translat.e'), edit_element_path(@child) %>)
<%  end %>
<br/>